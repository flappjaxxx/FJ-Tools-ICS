chmod 755 /system/xbin/andrev_oc
killall andrev_oc
echo 'powersave' > /system/etc/andrev_oc/sleep_gov
echo '192000' > /system/etc/andrev_oc/sleep_max
echo '192000' > /system/etc/andrev_oc/sleep_min
echo 'ondemand' > /system/etc/andrev_oc/wake_gov
echo '1512000' > /system/etc/andrev_oc/wake_max
echo '192000' > /system/etc/andrev_oc/wake_min
rm  /system/etc/init.d/84fj
rm  /system/etc/init.d/84fjwmax
rm  /system/etc/init.d/84fjwmin
rm  /system/etc/init.d/84fjwgov
echo '#!/system/bin/sh' > /system/etc/init.d/84fj
echo 'echo '192000' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq' > /system/etc/init.d/84fj
echo 'echo '1512000' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq' > /system/etc/init.d/84fj
echo 'echo 'ondemand' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor' > /system/etc/init.d/84fj
chmod 777 /system/etc/init.d/84fj
/system/etc/init.d/84fj
chmod 777 /system/etc/init.d/83aroc
/system/etc/init.d/83aroc
echo 'flappjaxxx' > /system/media/profile
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
echo flappjaxxx Custom Profile!
echo Sleep = powersave 192/192
echo Wake = ondemand 192/1512
}

