novalauncher() {
FILE=NovaLauncher.apk
MD5SUM=c27eb1137d21af9907e5241512ad3cec
URL="http://rootdev.org/i717/TU2/updates/NovaLauncher.apk"
if [ ! -d /sdcard/tmp ]; then
  MK=1
  mkdir -p /sdcard/tmp
fi
cd /sdcard/tmp
if [ -f "$FILE" ]; then
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" != "1" ]; then
echo "Downloading Package Be Patient"
  rm  "$FILE"
  wget "$URL"
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" = "1" ]; then
echo "Mounting File System"
  mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
  cd /
echo "Copying NovaLauncher to /system/app"
  cp /sdcard/tmp/NovaLauncher.apk /system/app/NovaLauncher.apk
echo "Setting Permissions"
  chmod 644 /system/app/NovaLauncher.apk
  mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
  cd - >/dev/null 2>&1
  echo "Done! Enjoy NovaLauncher!"
else
  echo "Failed to download / install NovaLauncher"
  exit 1
fi
rm  /sdcard/tmp/$FILE

if [ "$MK" = "1" ]; then
  rmdir /sdcard/tmp 
fi
}

