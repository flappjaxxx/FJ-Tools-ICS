samsnote() {
FILE=Snote.apk
MD5SUM=dfa86c2b4c4391ff5fd0d34921ca0572
URL="http://rootdev.org/i717/TU2/updates/Snote.apk"
if [ ! -d /sdcard/tmp ]; then
  MK=1
  mkdir -p /sdcard/tmp
fi
cd /sdcard/tmp
if [ -f "$FILE" ]; then
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" != "1" ]; then
echo "Downloading Package Be Patient -60MB-"
  rm  "$FILE"
  wget "$URL"
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" = "1" ]; then
echo "Mounting File System"
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
#    cd /
#  echo "Removing Obsolete Files..."
#  rm /data/app/*netflix*
#  rm /data/app/*Netflix*
#  echo "Copying Netflix-1.7-Black-b3 to /data/app"
#    cp /sdcard/tmp/Netflix-1.7-Black-b3.apk /data/app/Netflix-1.7-Black-b3.apk
#  echo "Setting Permissions"
#    chmod 644 /data/app/Netflix-1.7-Black-b3.apk
#    mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p25 /data
#    cd - >/dev/null 2>&1
echo "Installing Samsung S-Note"
cd /sdcard/tmp
cp Snote.apk /system/app/Snote.apk
echo "Samsung S-Note Successfully Installed"
echo "Please Reboot to ensure proper execution"
else
  echo "Failed to download / install Samsung S-Note"
  exit 1
fi
rm  /sdcard/tmp/$FILE

if [ "$MK" = "1" ]; then
  rmdir /sdcard/tmp 
fi
}

svoice() {
FILE=svoice.tgz
MD5SUM=73dd516be224e8bfa0df7000c1f6ec92
URL="http://rootdev.org/i717/TU2/updates/svoice.tgz"
if [ ! -d /sdcard/download/tmp ]; then
  MK=1
  mkdir -p /sdcard/download/tmp
fi
cd /sdcard/download/tmp
if [ -f "$FILE" ]; then
MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" != "1" ]; then 
echo "Downloading Package Be Patient"
rm  "$FILE"
wget "$URL"
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" = "1" ]; then
echo "Mounting File System"
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
  cd /sdcard/download/tmp
echo "Extracting Package"
  tar -xzf svoice.tgz
echo "Removing Old Files"
rm /data/app/com.vlingo.midas*.apk
echo "Copying Files"
  cp -R system /
echo "Setting Permissions"
chmod 644 /system/app/voicetalk.apk
